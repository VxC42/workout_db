<table>
    <tr><td>Name</td><td>Reps</td><td>Weight</td><td>Date</td><td>Units</td><td>Options</td></tr>
    {{#each results}}
    <tr>

        <td>{{this.name}}</td>
        <td>{{this.reps}}</td>
        <td>{{this.weight}}</td>
        <td>{{this.date}}</td>
        <td>{{this.lbs}}</td>
        <td><form action="/edit" method="get"><input type="submit" name={{this.id}} value ="Delete"> <input type="submit" name="Update" value="Update"></form></td>
    </tr>
    {{/each}}
</table>
