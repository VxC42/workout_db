<div id="form_container">
    <Update {{this.name}}
    <form action="/edit" method="get">
        <label for "name"> Name of Exercise:</label>
        <input type="text" name="name" class="input_field" >
        <label for "rep"> Number of Reps:</label>
        <input type="text" name="reps" class="input_field" >
        <label for "weight"> Amount of Weight:</label>
        <input type="text" name="weight" class="input_field" >
        <label for "date"> Date (MM-DD-YYYY):</label>
        <input type="text" name="date" class="input_field" >
        <label for "lbs"> Units used:</label>
        <select name="lbs" class="dropdown" >
            <option value=""></option>
            <option value="lbs">lbs</option>
            <option value="kgs">kgs</option>
            </select>
        <input type="submit" class="submit_btn" name="Save" value="Save">
    </form>

</div>
